{
  "name": "OSMKit",
  "version": "0.1",
  "license": {
    "type": "MIT"
  },
  "homepage": "https://github.com/davidchiles/OSMKit",
  "authors": {
    "David Chiles": "dwalterc@gmail.com"
  },
  "summary": "OpenStreetMap library for iOS and OS X",
  "source": {
    "git": "https://github.com/davidchiles/OSMKit",
    "tag": "v0.2"
  },
  "requires_arc": true,
  "platforms": {
    "ios": "7.0"
  },
  "dependencies": {
    "AFNetworking": [
      "~> 2.6"
    ],
    "TBXML": [
      "~> 1.5"
    ],
    "gtm-oauth": [
      "~> 0.0"
    ],
    "KissXML": [
      "~> 5.0"
    ]
  },
  "xcconfig": {
    "HEADER_SEARCH_PATHS": "/usr/include/libxml2"
  },
  "source_files": [
    "OSMKit/*.{h,m}",
    "OSMKit/**/*.{h,m}",
    "OSMKit/**/**/*.{h,m}"
  ],
  "public_header_files": [
    "OSMKit/*.{h}",
    "OSMKit/**/*.{h}",
    "OSMKit/**/**/*.{h}"
  ]
}
